{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Gateway": {
    "BaseUrl": "http://localhost:7100/"
  },
  "TokenManagement": {
    "RefreshIntervalMinutes": 14,
    "ExpiryWarningMinutes": 2,
    "CheckBeforeApiCallMinutes": 2
  },
  "AllowedHosts": "*"
}
